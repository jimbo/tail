{
  "name": "tail",
  "version": "1.0.0",
  "description": "Tailwind sandbox",
  "main": "index.js",
  "author": "Jimmy Sanford",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack --mode=production",
    "clean": "rm -rf dist",
    "format": "prettier --write '@(src|theme)/**/*.@(css|js|jsx)' '*.js'",
    "serve": "NODE_ENV=production webpack serve --mode=production",
    "start": "NODE_ENV=development webpack serve --mode=development"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.0.1",
    "postcss": "^8.2.4",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.1.1",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^18.0.0-alpha-3e8c86c1c-20210630",
    "react-dom": "^18.0.0-alpha-3e8c86c1c-20210630",
    "react-helmet-async": "^1.0.9"
  },
  "sideEffects": [
    "*.css"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "singleQuote": false,
    "quoteProps": "consistent",
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always"
  }
}
